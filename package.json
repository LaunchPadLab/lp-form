{
  "name": "@launchpadlab/lp-form",
  "version": "1.0.0",
  "description": "Extensions for the reduxForm HOC",
  "main": "lib/index.js",
  "scripts": {
    "start": "yarn run build:development",
    "build": "mkdir -p lib && babel src --out-dir lib --no-comments --minified",
    "build:development": "mkdir -p lib && babel src --watch --out-dir lib",
    "clean": "rimraf lib",
    "docs": "documentation build src/index.js -f md -o docs.md",
    "lint": "eslint src",
    "precommit": "yarn run docs && git add docs.md",
    "prepublish": "yarn run lint && yarn run clean && yarn run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": "launchpadlab/lp-form",
  "keywords": [
    "redux",
    "form"
  ],
  "author": "dpikt",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib/"
  ],
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "codeclimate-test-reporter": "^0.5.0",
    "documentation": "^4.0.0-beta.18",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "react": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "redux-form": "^6.0.0",
    "react": "^15.5.0"
  },
  "dependencies": {
    "@launchpadlab/lp-utils": "^2.12.3",
    "is-promise": "^2.1.0",
    "lodash": "^4.17.4",
    "validate.js": "^0.11.1"
  }
}
